{
  "name": "redux-duck-immer",
  "version": "1.1.0",
  "description": "Providing redux helpers to implement ducks-modular-redux proposal and Immer for immutability",
  "repository": "git@github.com:cjies/redux-duck-immer.git",
  "author": "CJies <cijies@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/duck.js",
  "module": "./dist/duck.module.js",
  "types": "./dist/duck.d.ts",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c && cpx \"src/*.{d.ts,js.flow}\" dist --verbose",
    "test": "NODE_ENV=test ava"
  },
  "dependencies": {
    "immer": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "cpx": "^1.5.0",
    "flow-bin": "^0.79.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.9.3",
    "rollup-plugin-babel": "^4.3.2"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": "@babel/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
